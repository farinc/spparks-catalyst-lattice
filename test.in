seed 12345
app_style surface/rxn        # defaults k1=k2=k3=1
solve_style tree             # rejection-free KMC

dimension 2
lattice sq/4n 1.0
region box block 0 64 0 64 -0.5 0.5
create_box box
create_sites box

# 0 = *, 1 = A*, 2 = B*
set site range 0 0
set site fraction 1 0.25 1001
set site fraction 2 0.25 1002

stats 0.01
run 100.0